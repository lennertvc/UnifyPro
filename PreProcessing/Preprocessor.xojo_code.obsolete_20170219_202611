	#tag Method, Flags = &h0
		Sub collectProjects(baseFolder as Folderitem)
		  
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h0
		Sub updateProjects()
		  
		End Sub
	#tag EndMethod


	#tag ComputedProperty, Flags = &h0
		#tag Getter
			Get
			  dim projectFiles() as FolderItem
			  
			  for i as integer = 1 to sourceFolder.Count
			    fileArray.Append(sourceFolder.item(i))
			  next
			  
			  return projectFiles
			End Get
		#tag EndGetter
		projectFiles As Integer
	#tag EndComputedProperty

	#tag ComputedProperty, Flags = &h0
		sourceFolder As folderItem
	#tag EndComputedProperty


	#tag ViewBehavior
		#tag ViewProperty
			Name="Index"
